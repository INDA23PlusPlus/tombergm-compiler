fn ack(m, n)
{
	if (m == 0)
	{
		return n + 1;
	}
	else if (n == 0)
	{
		return ack(m - 1, 1);
	}
	else
	{
		return ack(m - 1, ack(m, n - 1));
	}
}

fn main()
{
	let m = 0;
	let n = 0;

	while (1)
	{
		putcs('ack(');
		putd(m);
		putcs(', ');
		putd(n);
		putcs('):\t');
		print(ack(m, n));

		n = n + 1;
		if (n == 6)
		{
			m = m + 1;
			n = 0;
		}
	}
}
