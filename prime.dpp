fn prime(n)
{
	let m = 3;
	let k = sqrt(n);

	while (m <= k)
	{
		if (n % m == 0)
		{
			return 0;
		}

		m = m + 2;
	}

	return 1;
}

fn main()
{
	let n = 3;
	let p = 1;

	print(2);

	while (n < 100000)
	{
		if (prime(n))
		{
			putcs('p(');
			putd(p);
			putcs('):\t');
			print(n);

			p = p + 1;
		}

		n = n + 2;
	}
}
